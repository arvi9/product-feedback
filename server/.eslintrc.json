{
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base",
    "@onosendi/eslint-config/lib/typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "func-names": "off",
    "prefer-arrow-callback": "off"
  }
}

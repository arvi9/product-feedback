{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "max-len": "off",
        "no-console": [1, { "allow": ["warn", "error"]}],
        "react/jsx-props-no-spreading": "off",
        "react/react-in-jsx-scope": "off"
    }
}

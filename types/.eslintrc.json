{
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "@onosendi/eslint-config/lib/typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "func-names": "off",
    "prefer-arrow-callback": "off"
  }
}
